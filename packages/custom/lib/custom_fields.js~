// Custom Post Field

Posts.addField({
  fieldName: 'impact',
  fieldSchema: {
    label: "Impact",
    min: 0,
    max: 2,
    defaultValue: 0,
    type: Number,
    optional: true,
    editableBy: ["member", "admin"]
  }
});

