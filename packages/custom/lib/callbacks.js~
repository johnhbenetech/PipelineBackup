function pipelineCal (post) {
   alert(4*(post.impact+post.need+post.missionfit+post.legality)+3*(post.currentmoney+post.futuremoney)+2*(post.talent+post.technicalrisk+post.communityofusers+post.communityofpartners+post.portfoliofit)+post.cost+post.internalchampion+post.exitstrategies+post.existingdata);
   return post;}

Telescope.callbacks.add("postSubmitClient", pipelineCal);
